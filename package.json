{
    "name": "ng2-pica",
    "version": "1.2.6",
    "description": "Angular wrapper for pica to resize images.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bergben/ng2-pica.git"
    },
    "license": "MIT",
    "module": "./dist/ng2-pica.js",
    "main": "./dist/bundles/ng2-pica.umd.js",
    "typings": "./dist/ng2-pica.d.ts",
    "scripts": {
        "build": "npm run transpile && npm run package && npm run minify",
        "minify": "uglifyjs dist/bundles/ng2-pica.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ng2-pica.umd.min.js",
        "package": "rollup -c",
        "transpile": "ngc"
    },
    "author": {
        "name": "Benedikt Berger",
        "email": "benedikt.berger@live.de"
    },
    "keywords": [
        "angular",
        "angular2",
        "pica",
        "image",
        "resize"
    ],
    "dependencies": {
        "exif-js": "^2.3.0",
        "pica": ">=4.1.1"
    },
    "peerDependencies": {
        "@angular/core": ">=6.0.0",
        "@angular/common": ">=6.0.0",
        "@angular/platform-browser": ">=6.0.0",
        "rxjs": ">=6.0.0"
    },
    "devDependencies": {
        "@angular/common": ">=6.0.0",
        "@angular/compiler": ">=6.0.0",
        "@angular/compiler-cli": ">=6.0.0",
        "@angular/core": ">=6.0.0",
        "@angular/platform-browser": ">=6.0.0",
        "@angular/platform-browser-dynamic": ">=6.0.0",
        "@angular/router": ">=6.0.0",
        "@types/node": ">=8.0.0",
        "core-js": ">=2.5.4",
        "reflect-metadata": "^0.1.12",
        "rollup": "^0.62.0",
        "rxjs": "6.0.0",
        "tslint": "^4.4.2",
        "typescript": "2.7.2",
        "uglify-js": "^3.4.4",
        "zone.js": "^0.8.26"
    },
    "bugs": {
        "url": "https://github.com/bergben/ng2-pica/issues"
    },
    "homepage": "https://github.com/bergben/ng2-pica#readme"
}
